{
  "name": "starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:agent": "mastra dev",
    "dev:ui": "next dev --turbopack",
    "dev:debug": "LOG_LEVEL=debug npm run dev",
    "dev:mcp": "ts-node --esm src/mcp-server/index.ts",
    "build:ui": "next build",
    "start:ui": "next start",
    "build:agent": "mastra build",
    "start:agent": "mastra start",
    "build:mcp": "tsc -p tsconfig.mcp.json",
    "start:mcp": "node dist/mcp-server/index.js",
    "build:api": "tsc -p tsconfig.api.json",
    "start:api": "node dist/api/index.js",
    "cron": "node cron.js",
    "lint": "next lint",
    "build": "mastra build && next build",
    "start": "concurrently \"npm:start:agent\" \"npm:start:ui\"",
    "postinstall": "prisma generate && prisma migrate deploy"
  },
  "dependencies": {
    "@ag-ui/client": "^0.0.39",
    "@ag-ui/mastra": "0.1.0",
    "@ai-sdk/mistral": "^2.0.19",
    "@ai-sdk/openai": "^2.0.52",
    "@copilotkit/react-core": "1.10.6",
    "@copilotkit/react-textarea": "1.10.6",
    "@copilotkit/react-ui": "1.10.6",
    "@copilotkit/runtime": "1.10.6",
    "@libsql/client": "^0.15.15",
    "@mastra/client-js": "0.14.1",
    "@mastra/core": "0.19.1",
    "@mastra/libsql": "0.15.0",
    "@mastra/mcp": "^0.13.5",
    "@mastra/memory": "0.15.4",
    "@prisma/client": "^6.17.1",
    "ai": "^5.0.76",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.63.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "envalid": "^8.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-slow-down": "^3.0.0",
    "framer-motion": "^12.23.24",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "libsql": "^0.5.22",
    "lucide-react": "^0.548.0",
    "mastra": "0.13.4",
    "next": "^15.5.5",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.9",
    "ollama-ai-provider-v2": "^1.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "resend": "^6.2.2",
    "sanitize-html": "^2.17.0",
    "serpapi": "^2.2.1",
    "sooner": "^1.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "winston": "^3.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.21",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
